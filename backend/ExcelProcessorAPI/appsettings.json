{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=ExcelProcessorDB;Trusted_Connection=true;TrustServerCertificate=true;",
    "ProductionConnection": "Data Source=YOUR_SERVER;Initial Catalog=ExcelProcessorDB;Integrated Security=True;TrustServerCertificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "ExcelProcessor": {
    "MaxFileSize": 52428800,
    "MaxSheets": 50,
    "MaxRowsPerSheet": 10000,
    "SupportedFileTypes": [".xlsx", ".xls"],
    "BulkInsertBatchSize": 1000
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "https://localhost:3000",
      "https://localhost:5173"
    ]
  }
}